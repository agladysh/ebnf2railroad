{
  "name": "ebnf2railroad",
  "version": "1.4.0",
  "description": "EBNF to Railroad diagram",
  "keywords": [
    "ebnf",
    "context-free grammar",
    "railroad",
    "railroad diagram",
    "syntax diagram",
    "diagram",
    "documentation",
    "generator",
    "cli"
  ],
  "main": "src/main.js",
  "repository": "git@github.com:matthijsgroen/ebnf2railroad.git",
  "author": "Matthijs Groen <matthijs@kabisa.nl>",
  "license": "MIT",
  "bin": {
    "ebnf2railroad": "./bin/ebnf2railroad.js"
  },
  "scripts": {
    "build-parser": "jison src/ebnf.jison -o src/ebnf-parser.js",
    "test": "mocha",
    "lint": "eslint src/ test/",
    "update-examples": "bin/ebnf2railroad.js examples/json.ebnf --title JSON; bin/ebnf2railroad.js examples/ebnf.ebnf --title EBNF; bin/ebnf2railroad.js examples/optimizations.ebnf"
  },
  "private": false,
  "dependencies": {
    "commander": "^2.19.0",
    "showdown": "^1.8.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jison": "^0.4.18",
    "mocha": "^5.2.0",
    "prettier": "^1.14.3",
    "railroad-diagrams": "https://github.com/tabatkins/railroad-diagrams"
  }
}
